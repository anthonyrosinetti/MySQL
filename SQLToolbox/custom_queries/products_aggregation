	select
		split(case when (website_ids like '%1%' and website_ids like '%4%') then 'LCDA,Dogtore IT' when website_ids like '%1%' then 'LCDA' when website_ids like '%4%' then 'Dogtore IT' else '' end,',') as store,
  		product_id,
		product_sku,
  		product_name,
		is_product_active,
	    surtype,
    	type,
	    manufacturer,
    	brand,
  		is_brand_active
  	from
		`reporting-boryl-lcda.analysis.analysis_products`
